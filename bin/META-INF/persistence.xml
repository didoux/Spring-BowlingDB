<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	
	
		<persistence-unit name="bowling" transaction-type="JTA" >
			<description>generated-persistence-unit</description>
					<provider>org.hibernate.ejb.HibernatePersistence</provider>
      			<class>bowling.domain.Week</class>
      			<class>bowling.domain.Team</class>
      			<class>bowling.domain.Player</class>
      			<class>bowling.domain.League</class>
      			<class>bowling.domain.Frame</class>
      			<class>bowling.domain.Throw</class>
      			<class>bowling.domain.Game</class>
   			<exclude-unlisted-classes>true</exclude-unlisted-classes>
		</persistence-unit>

	
</persistence>
